{
  "name": "@spectrum/spectrum-css-grid",
  "version": "0.3.0",
  "description": "CSS Grid specs as defined in Spectrum DNA",
  "style": "dist/spectrum-css-grid.css",
  "main": "dist/spectrum-css-grid.css",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "run-s clean build:*",
    "build:gulp": "gulp",
    "build:node": "node --experimental-modules ./scripts/node-example.mjs",
    "help": "convertDnaVars --help",
    "build:cmd": "cowsay 'starting balthazar from command line...' && convertDnaVars -d ./dist/cmd/scss/ --type sass --prefix _",
    "bump": "dna-npm-bump-version",
    "release": "npm run build && npm run bump",
    "test": "echo 'nope' || exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git@git.corp.adobe.com:spectrum/spectrum-css-grid.git"
  },
  "keywords": [
    "spectrum",
    "grid",
    "css"
  ],
  "author": "Grp-AD-Spectrum-Team <Grp-AD-Spectrum-Team@adobe.com>",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "@spectrum/balthazar": "^3.0.1",
    "@spectrum/dna-npm-release": "^2.1.0",
    "@spectrum/spectrum-dna": "^5.9.0",
    "del": "^3.x",
    "fancy-log": "^1.3.2",
    "gulp": "^4.0.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-pug": "^4.0.1",
    "gulp-rename": "^1.x",
    "gulp-replace": "^0.x",
    "gulp-rimraf": "^0.2.2",
    "gulp-sass": "^4.x",
    "gulp-sourcemaps": "^2.6.5",
    "npm-run-all": "^4.1.5",
    "pug": "^2.0.3",
    "rimraf": "^2.6.3",
    "run-sequence": "^2.x"
  }
}

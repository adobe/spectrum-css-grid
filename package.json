{
  "name": "@spectrum/spectrum-css-grid",
  "version": "0.2.0",
  "description": "CSS Grid specs as defined in Spectrum DNA",
  "style": "dist/spectrum-css-grid.css",
  "main": "dist/spectrum-css-grid.css",
  "scripts": {
    "build": "gulp",
    "build:origins": "balthazar --config ./balthazar-config.json --origin ./node_modules/@spectrum/spectrum-origins/src/ --destination ./src/spectrum-origins/ --type sass",
    "bump": "dna-npm-bump-version",
    "release:snapshot": "run-s build bump publish:snapshot",
    "release:stable": "run-s build bump publish:stable",
    "publish:snapshot": "dna-npm-publish --releaseAsType=\"snapshot\"",
    "publish:stable": "dna-npm-publish --releaseAsType=\"stable\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@git.corp.adobe.com:spectrum/spectrum-css-grid.git"
  },
  "author": "Grp-AD-Spectrum-Team <Grp-AD-Spectrum-Team@adobe.com>",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "@spectrum/balthazar": "^1.1.x",
    "@spectrum/dna-npm-release": "^1.2.x",
    "@spectrum/spectrum-origins": "^1.2.x",
    "gulp": "^3.9.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-pug": "^3.3.0",
    "gulp-rimraf": "^0.2.1",
    "gulp-run": "^1.7.1",
    "gulp-sass": "^2.0.4",
    "gulp-sourcemaps": "^1.3.0",
    "npm-run-all": "^4.1.1",
    "pug": "^2.0.0-rc.2",
    "rimraf": "^2.6.1"
  }
}
